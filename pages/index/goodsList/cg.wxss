.swiper_group {
  height: 428rpx;
  width: 100%;
}
.search_group .swiper-item {
  width: 100%;
  height: 100%;
  display: inline-flex;
  align-items: center;
  justify-content: center;
}
.swiperImg {
  width: 93%;
  border-radius: 6%;
  height: 410rpx;
}

.index_tab .bottom_scroll {
  width: 100%;
  white-space: nowrap;
  overflow: hidden;
  line-height: 50rpx;
  margin: 28rpx 0;
}

.index_tab .bottom_scroll .nav {
  min-width: 117rpx;
  display: inline-block;
  white-space: normal;
  text-align: center;
  font-size: 28rpx;
  height: 50rpx;
  border-radius: 25rpx;
  background: #fafafa;
  color: #989898;
  margin-right: 16rpx;
  padding: 0 16rpx;
}

.index_tab .bottom_scroll .nav:last-child {
  margin-right: 30rpx;
}

.index_tab .bottom_scroll .nav:first-child {
  margin-left: 30rpx;
}


.index_tab .bottom_scroll .nav.cur {
  background: #ff7f11;
  color: #fff;
}

.search_group {
  margin: 0 30rpx;
  padding: 5rpx 30rpx;
  display: inline-flex;
  align-items: center;
  width: 690rpx;
  height: 66rpx;
  background: #f6f6f6;
  border-radius: 33rpx;
}

.times {
  width: 690rpx;
  margin: 0 auto;
  padding: 30rpx 0 0;
  align-items: center;

}

.search_icon {
  width: 28rpx;
  height: 30rpx;
  margin-right: 20rpx;
}

.shopBox {
  padding: 30rpx;
}

.w1 .commodityName {
  width: 462rpx;
  margin: 35rpx 0 46rpx 0;
  font-size: 30rpx;
}
.w1 .cover {
  width: 187rpx;
  height: 197rpx;
  float: left;
  margin: 14px 8px 0 9px;
}
.w1 .right {
  padding: 35rpx 0 29rpx;
  width: calc(100% - 206rpx);
}
.w2 {
  width: 336rpx;
  margin-right: 20rpx;
  flex-direction: column;
  margin-bottom: 22rpx;
}
.w2 .cover {
  width: 336rpx;
  height: 192rpx;
  margin-bottom: 13rpx;
}
.w3 {
  width: 219rpx;
  margin-right: 20rpx;
}
.shopList {
  height: 354rpx;
  background: #ffffff;
  border-radius: 14rpx;
  box-shadow: 0px 3px 16px 0px rgba(0, 0, 0, 0.06);
  display: inline-flex;
  /* padding: 6rpx; */
  overflow: hidden;
  /* margin-bottom: 20rpx; */
}
.w1 {
  width: 690rpx;
  height: 248rpx;
  margin-bottom: 24rpx;
}
.sw1 {
  height: auto;
  box-shadow: none;
  margin-bottom: 0;
}
.skillNum {
  top: 153rpx;
  left: 0;
