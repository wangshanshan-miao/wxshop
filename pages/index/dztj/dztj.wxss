/* pages/index/dztj/dztj.wxss */
/* 搜索 */
.produce_top {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  width: 100%;
  height: 160rpx;
  background: #009DFF;
  padding: 40rpx 30rpx 0 30rpx;
}

.produce_searchInput {
  width: 100%;
  height: 65rpx;
  line-height: 65rpx;
  padding: 0rpx 74rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 35rpx;
}

.produce_search_del {
  width: 25rpx;
  height: 25rpx;
  position: absolute;
  right: 30rpx;
  top: 22rpx;
}

.produce_search_icon {
  width: 22rpx;
  height: 22rpx;
  position: absolute;
  left: 30rpx;
  top: 22rpx;
}

/* 商品 */

.content {
  margin: 180rpx auto 0 ;
  width: 690rpx;
  flex-wrap: wrap;
  padding: 0 30rpx;
}

.goodsCell {
  width: 48%;
  border-radius: 20rpx;
  box-shadow: 0px 3px 6px rgba(0, 0, 0, 0.11);
  margin-bottom: 40rpx;
}

.goodImg {
  box-sizing: border-box;
  width: 100%;
  height: 305rpx;
  /* background-color: aquamarine; */
}

.good_buyCar{
  width: 50rpx;
  height: 50rpx;
  flex-shrink: 0;
}

.car_group{
  width: 100%;
  box-sizing: border-box;
}