/* pages/self/join/index.wxss */
.header {
  width: 750rpx;
  height: 68rpx;
  line-height: 68rpx;
  background: rgba(242, 242, 242, 1);
  color: #D32E32;
}

.list {
  margin: 0 auto;
  width: 700rpx;
  box-sizing: border-box;
  height: 105rpx;
  border-bottom: 1px solid #E2E1E4;
}

.list>input {
  text-align: right;
  /* color: #B9B9B9; */
}

.title1 {
  margin: 30rpx;
}

.logo {
  width: 204rpx;
  height: 200rpx;
  background-color: #F4F1F4;
  margin-left: 26rpx;
}

.logo>.img{
  width: 100%;
  height: 100%;
} 

.icon {
  width: 54rpx;
  height: 45rpx;
}

.icon1{
  position: absolute;
  transform: translate(-50%,-50%);
  top: 50%;
  left: 50%;
}

.photo {
  position: relative;
  flex-wrap: wrap;
  justify-content: space-between;
  margin: 0 auto;
  width: 696rpx;
  height: 346rpx;
  background: rgba(242, 242, 242, 1);
}

.photo>.img{
  width: 100%;
  height: 100%;
} 

textarea {
  margin: 0 auto;
  width: 699rpx;
  height: 328rpx;
  /* color: #CBCBCB; */
  padding: 24rpx;
  box-sizing: border-box;
  border:1rpx solid rgba(203,203,203,1);
}

.button {
  line-height: 88rpx;
  margin: 100rpx auto;
  width: 568rpx;
  height: 88rpx;
  background: rgba(0, 157, 255, 1);
  border-radius: 210rpx;
}

.picker{
  color: #B9B9B9 !important;
}

.imgs{
  width: 212rpx;
  height: 100rpx;
}

.imgs>.realPicture{
  width: 212rpx;
  height: 100rpx;
}