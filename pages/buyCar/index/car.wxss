.line1{
    position: fixed;
    top: 0;
    z-index: 999;
    height: 2px;
    background-color: #fafafa;
    width: 100vw;
}
.carModel {
    width: 100%;
    margin-bottom: 130rpx;
    /* height: 100%; */
    overflow: hidden;
    padding-top: 38rpx;
}

.buycarOut {
    width: 100%;
    height: calc(100% - 135rpx);
    overflow: auto;
    -webkit-overflow-scrolling: touch;
}

.carModel_top {
    /* padding: 20rpx 30rpx 0 30rpx; */
    box-sizing: border-box;
    padding-right: 30rpx;
    line-height: 60rpx;
    text-align: right;
    background-color: #fff;
    position: fixed;
    top: 2px;
    z-index: 99;
    width: 100%;
}

.shopModle_list_cell {
    padding: 0 30rpx;
}

.shopModle_list_top_left .shop_icon {
    width: 26.58rpx;
    height: 25.05rpx;
}

.shopModle_list_top_name {
    margin: 0 15rpx;
}

.shopModle_list_top_left .goshopIcon {
    width: 18.38rpx;
    height: 18.38rpx;
}

.chose_buyGood {
    width: 34rpx;
    height: 34rpx;
    flex-shrink: 0;
    margin-top: 60rpx;
}

.buyGood_coverimg {
    width: 162rpx;
    height: 157rpx;
    /* border: 20rpx;
    border-radius: 20rpx; */
    flex-shrink: 0;
    margin: 0 30rpx 0 24rpx;
    /* background-color: aquamarine; */
}

.buyGood_list {
    margin-bottom: 61rpx;
}

.buyGood_name {
  margin-bottom: 14rpx;
}
.buyGood_list_right {
    width: 100%;
}

.buyGood_numOpa {
    height: 40rpx;
    background: #ffffff;
    border: 1px solid rgba(220,220,220,0.9);
    border-radius: 8rpx;
    width: 160rpx;
}

.buyGood_btn {
    width: 47rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    opacity: 1;
    border-right: 1rpx solid #dcdcdc;
}

.reace_buyNum {
    border-radius: 4px 0px 0px 4px;
}

.add_buyNum {
    border-radius: 0px 4px 4px 0px;
    border-right: none;
}

.buyGoodNum {
    width: 64rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
}

.settlement {
    width: 100%;
    height: 128rpx;
    padding: 0 30rpx;
    position: fixed;
    bottom: 0;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0px -6px 14px rgba(0, 0, 0, 0.06);
}

.choseAll_buyGood {
    margin-left: 24rpx;
}

.allMoney {
    margin-left: 32rpx;
    /* align-items: center; */
}

.money_num {
    color: #D32E32;
}

.settlement_left {
  width: calc(100% - 300rpx);
}

.settlement_right {
    width: 237rpx;
    height: 84rpx;
    line-height: 84rpx;
    background: linear-gradient(129deg,#fca153 10%, #fb8a29 96%);
    border-radius: 42rpx;
    opacity: 1;
}


.selectAll{
    margin-top: 0 ;
}

