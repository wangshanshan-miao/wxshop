.coupon {
    width: 697rpx;
    height: 205rpx;
    margin: 0rpx auto;
    box-sizing: border-box;
    overflow: hidden;
    position: relative;
    margin-top: 20rpx;
    border-radius: 5rpx;
}

.coupon>image {
    position: absolute;
    z-index: 1;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 205rpx;
}

.coupon>.left {
    position: absolute;
    z-index: 2;
    margin-left: 70rpx;
    margin-right: 57rpx;
    color: #fff;
    font-size: 66rpx;
}

.coupon>.left>text {
    font-size: 26rpx;
}

.coupon>.right {
    position: absolute;
    z-index: 2;
    line-height: 53rpx;
    right: 31rpx;
    width: 380rpx;
}

.coupon>.right>.top>.top-left {
    width: 67rpx;
    height: 28rpx;
    font-size: 17rpx;
    background: linear-gradient(180deg, rgba(0, 144, 255, 1) 0%, rgba(78, 178, 255, 1) 100%);
    opacity: 1;
    border-radius: 5rpx;
}

.coupon>.right>.top>.top-right {
    margin-left: 215rpx;
    color: #B7B7B7;
}

.coupon>.right>.middle {
    margin: 10rpx 0;
    color: #484A59;
    line-height: 36rpx;
}

