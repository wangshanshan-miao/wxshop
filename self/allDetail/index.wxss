.coupons {
    position: fixed;
    z-index: 99;
    bottom: 0;
    width: 100%;
    height: 900rpx;
    background-color: #fff;
    border-top-left-radius: 40rpx;
    border-top-right-radius: 40rpx;
    border: 1px solid #ccc;
    box-sizing: border-box;
    overflow: hidden;
}

@keyframes sliderUp {
    from {
        transform: translateY(900rpx);
        opacity: 0;
    }

    to {
        transform: translateY(0);
        opacity: 1;
    }
}

.sliderUp {
    animation: sliderUp 0.5s ease both;
}

.coupons .title {
    font-weight: 500;
    width: 100%;
    height: 100rpx;
    background-color: #fff;
    position: absolute;
    top: 0;
    z-index: 22;
}

.close {
    width: 24rpx;
    height: 24rpx;
    top: 42rpx;
    right: 42rpx;
    z-index: 99;
}

.scroll-y {
    margin-top: 100rpx;
    width: 100%;
    height: 800rpx;
}

.mask {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 9;
    width: 100%;
    height: 100vh;
    background-color: black;
    opacity: 0.5;
}

.coupon {
    width: 697rpx;
    height: 205rpx;
    margin: 0rpx auto;
    box-sizing: border-box;
    overflow: hidden;
    position: relative;
    margin-top: 20rpx;
    border-radius: 5rpx;
}

.coupon .bg {
    position: absolute;
    z-index: -1;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 205rpx;
}

.coupon>.left {
    position: absolute;
    z-index: 2;
    margin-left: 70rpx;
    margin-right: 57rpx;
    color: #fff;
    font-size: 66rpx;
}

.coupon>.left>text {
    font-size: 26rpx;
}

.coupon>.right {
    position: absolute;
    z-index: 2;
    line-height: 53rpx;
    right: 31rpx;
    width: 380rpx;
    display: flex;
    justify-content: space-between;
    flex-direction: column;
}

.coupon>.right>.top>.top-left {
    width: 67rpx;
    height: 28rpx;
    font-size: 17rpx;
    background: linear-gradient(180deg, rgba(0, 144, 255, 1) 0%, rgba(78, 178, 255, 1) 100%);
    opacity: 1;
    border-radius: 5rpx;
}

.coupon>.right>.top>.top-right {
    margin-left: 215rpx;
    color: #B7B7B7;
}

.coupon>.right>.middle {
    margin: 10rpx 0;
    color: #484A59;
    line-height: 36rpx;
}


/* 物流 */
.logistics {
    position: fixed;
    bottom: 0;
    z-index: 9999;
    width: 100%;
    height: 421rpx;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0px -6px 14px rgba(0, 0, 0, 0.06);
    opacity: 1;
    border-radius: 18px 18px 0px 0px;
}

.logistics .close {
    width: 24rpx;
    height: 24rpx;
    top: 40rpx;
    right: 34rpx;
}

.logistics .title {
    border-bottom: 1px solid #E6E6E6;
    height: 96rpx;
    box-sizing: border-box;
    margin-bottom: 50rpx;
}

.logistics .list {
    width: 100%;
    box-sizing: border-box;
    padding: 0 40rpx;
    margin: 30rpx auto 30rpx;
}

@keyframes sliderUp {
    from {
        transform: translateY(421rpx);
    }

    to {
        transform: translateY(0);
    }
}

.sliderUp {
    animation: sliderUp 0.5s ease both;
}

