/* pages/self/address/update/index.wxss */
.space {
  width: 100%;
  height: 14rpx;
  background: rgba(246, 246, 246, 1);
  margin-bottom: 20rpx;
}

.list {
  margin: 0 auto;
  width: 700rpx;
  min-height: 105rpx;
  box-sizing: border-box;
  /* padding-right: 51rpx; */
  border-bottom: 1rpx solid #E2E2E2;
}

.list input {
  text-align: right;
  color: #222222;
  font-size: 28rpx !important;
}

.picker {
  color: #222222 !important;
}

.switch {
  margin: 30rpx auto;
  width: 700rpx;
  box-sizing: border-box;
  /* padding-right: 29rpx; */
}


.delete{
  margin-top: 30rpx;
  margin-left: 26rpx;
  color: #FF4600;
}
.button {
  margin: 116rpx auto 0;
  height: 88rpx;
  line-height: 88rpx;
  border-radius: 210rpx;
  width: 431rpx;
  background: linear-gradient(129deg, #fca153 10%, #fb8a29 96%);
}
.arrowLeft{
  width: 10rpx;
  height: 17rpx;
  margin-left: 30rpx;
}
.phone{
  color: #BFBFBF;
}
.fnum{
  display: inline-block;
}
.phone input{
  display: inline-block;
  width: 200rpx;
  margin-left: 10rpx;
}
textarea{
  padding: 32rpx 0;
  height: 100rpx;
  color: #222222;
}
page{
  background: #f6f6f6;
}
.bg{
  background-color: #fff;
}