page {
    width: 100%;
    height: auto;
}

.yhqTab {
    width: 100%;
    display: flex;
    height: 68rpx;
    box-sizing: border-box;
    position: fixed;
    z-index: 999;
    top: 0;
    background-color: #fff;
    padding: 14rpx;
    justify-content: space-around;
}

.choseYhqTab::after {
    display: block;
    content: '';
    width: 50rpx;
    height: 6rpx;
    background: rgba(251,148,103,1);
    opacity: 1;
    border-radius:3rpx;
    position: absolute;
    left: calc(50% - 23rpx);
    top: 47rpx;
}
.left{
  width: 170rpx;
  color: #FF784D;
  padding: 20rpx;
  box-sizing: border-box;
  /* border-right: 1px dashed #FF784D; */
}
.left .num{
  font-size: 55rpx;
  padding: 0;
}
.split-line {
  position: relative;
  flex: 0 0 0;
  border-left: 1px dashed #FF784D;
  margin: -2px 5px -1px 3px;
  background: #fff;
  height: 188rpx;
  top: 1px;
}
.split-line:before, .split-line:after {
  content: '';
  position: absolute;
  width: 16rpx;
  height: 8rpx;
  background: #fff;
  left: -9rpx;
  z-index: 1;
}
.split-line:before {
  border-radius: 0 0 8rpx 8rpx;
  top: 0;
}
.split-line:after {
  border-radius: 8rpx 8rpx 0 0;
  bottom: 0;
}
.couponBtn{
  width: 133rpx;
  height: 54rpx;
  line-height: 54rpx;
  background: #ff7f11;
  box-shadow: 0px 3px 10px 0px rgba(0,0,0,0.08); 
  text-align: center;
  border-radius: 54rpx;
  margin-right: 16rpx;
}

.yhqList {
  width: 690rpx;
  background: #ffffff;
  z-index: 9;
  padding: 30rpx;
  box-sizing: border-box;
  margin:  68rpx auto 0;
}

.yhqImgBg {
    width: 100%;
    height: 225rpx;
}

.yhqList_cell_con {
  background: #FEF5E4;
  margin-bottom: 24rpx;
  border: 1px solid #ff784d;
  border-radius: 10rpx;
}
.overyhq .yhqList_cell_con {
  background: #D5D5D5;
  border: 1px solid #d5d5d5;
}
.overyhq .split-line {
  border-left: 1px dashed #fff;
}

.yhq_money {
    align-items: flex-end;
    padding-top: 20rpx;
    width: 160rpx;
}

.yhq_dw {
    margin-right: 15rpx;
    position: relative;
    top: -21rpx;

}

.yhq_num {
    font-size: 66rpx;
}

.yhqIntro {
    margin-left: 23rpx;
    padding-top: 30rpx; 
}

.use_tj {
    margin: 16rpx 0;
}

.employ {
    margin-left: 10rpx;
    color: #009DFF;
    display: flex;
    height: 100%;
    flex-direction: column;
    justify-content: center;
}

.employ>view{
    text-align: center;
    width: 40rpx;
}

.gq_yhq {
    color: #999999;
}

.instructions {
    align-items: center;
    justify-content: center;
    width: 107rpx;
    height: 107rpx;
    background: linear-gradient(180deg, rgba(97, 180, 255, 1) 0%, rgba(0, 128, 243, 1) 100%);
    border-radius: 50%;
    opacity: 1;
    position: fixed;
    right: 20rpx;
    z-index: 99;
    bottom: 30rpx;
}

.con {
    width: 74rpx;
    height: 36rpx;
    right: 25rpx;
    transform: translate(0, -50%);
    top: 50%;
}

.con .img {
    width: 100%;
    height: 100%;
}


