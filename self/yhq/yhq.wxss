} */

.move {
    transform: translate(50px);
}

.modal {
    position: fixed;
    bottom: 0;
    z-index: 999;
    width: 750rpx;
    height: 130rpx;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0px -6rpx 14rpx rgba(0, 0, 0, 0.06);
    border-radius: 18rpx 18rpx 0px 0px;
}

.modal .all {
    /* border: 1rpx solid #707070; */
    border-radius: 50%;
    width: 34rpx;
    height: 34rpx;
    margin-left: 46rpx;
}

.modal .all .img {
    width: 100%;
    height: 100%;
}

.modal .text {
    margin-left: 29rpx;
}

.modal .delete {
    width: 414rpx;
    height: 88rpx;
    background: rgba(0, 157, 255, 1);
    opacity: 1;
    border-radius: 210px;
    margin-right: 25rpx;
}

.container1 {
    height: 60rpx;
    width: 100vw;
    box-sizing: border-box;
    background-color: #fff;
    position: fixed;
    z-index: 99;
    top: 68rpx;
    border-bottom: 1px solid #ccc;
}

.circle {
    /* border: 1rpx solid #707070; */
    /* border-radius: 50% !impor; */
    width: 34rpx !important;
    height: 34rpx !important;
    transform: translate(0, -50%) !important;
    top: 50% !important;
    left: -30px !important;
    /* box-sizing: border-box; */
}


.down {
    position: fixed;
    bottom: 0;
    width: 750rpx;
    height: 130rpx;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0px -6px 14px rgba(0, 0, 0, 0.06);
    opacity: 1;
    border-radius: 18px 18px 0px 0px;
}

.down .refund {
    width: 568rpx;
    height: 88rpx;
    background: rgba(0, 157, 255, 1);
    opacity: 1;
    border-radius: 210px;
}

.mask {
    width: 100vw;
    height: 100vh;
    position: fixed;
    top: 0;
    z-index: 99;
    background-color: #000;
    opacity: 0.5;
}


.name{
    height: 40rpx;
    box-sizing: border-box;
    line-height: 40rpx;
}


.tag {
    width: 67rpx;
    height: 28rpx;
    background: rgba(81, 177, 249, 1);
    opacity: 1;
    border-radius: 5px;
    color: #FFF;
    font-size: 17rpx;
    margin-right: 20rpx;
}
.overdate {
  color: #A4A4A4;
  background: #fff;
}
